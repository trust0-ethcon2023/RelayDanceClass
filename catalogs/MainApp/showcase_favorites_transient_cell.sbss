import themes.sbss

$_CONTENT_MARGIN = 10dp

#blank_cover {
    width: (1ph-$_CONTENT_MARGIN)*16/9;
    height: 1ph-$_CONTENT_MARGIN;
    position: absolute;
    gravity: left-top;
    color: $COLOR_DARK_WHITE;
}

#blank_line {
    width: 1pw;
    height: 1dp;
    position: absolute;
    gravity: bottom;
    color: $THEME_COLOR_LIGHT_TEXT;
}

/cell {
    skips-drawing: yes;
}
